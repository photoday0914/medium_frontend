CA,kBAAA,KAAA,EAAA,MAAA,CAAA,aADA;AAEA,kBAAA,OAAA,EAAA,MAAA,CAAA,QAFA;AAGA,kBAAA,QAAA,EAAA,MAAA,CAAA;AAHA,iBAJA,EASA;;AATA;AAAA,uBAUA,MAAA,CAAA,MAAA,CAAA,IAAA,CAAA,2CAAA,MAAA,EAAA,IAAA,CAVA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA,KAlCA;AAoCA,IAAA,MApCA,kBAoCA,IApCA,EAoCA;AACA,WAAA,KAAA,CAAA,MAAA,CAAA,KAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,EAAA,CAAA;AACA,WAAA,KAAA,sBAAA,KAAA,KAAA;AACA,KAvCA;AAwCA;AACA,IAAA,WAzCA,uBAyCA,MAzCA,EAyCA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,MAAA,CAAA,GAAA,CAAA,wCAAA,MAAA,CADA;;AAAA;AAAA;AACA,gBAAA,IADA,yBACA,IADA;AAEA,gBAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,gBAAA,MAAA,CAAA,KAAA,GAAA,IAAA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA;AA7CA;AAjDA,CAAA","sourcesContent":["<template>\r\n  <v-app>\r\n      <Header/>      \r\n      <v-container class=\"ma-15\" align=\"center\">\r\n        <v-row>\r\n            <v-col cols=\"12\" md=\"2\"></v-col>\r\n            <v-col cols=\"12\" md=\"8\">\r\n              <Bubble/>\r\n              <v-container>       \r\n                <h1>Your Story</h1>\r\n                <v-container>\r\n                  <v-row>\r\n                    <editor-menu-bar :editor=\"editor\" v-slot=\"{ commands, isActive }\">\r\n                        <div class=\"d-flex\">\r\n                          <v-container>\r\n                            <v-btn text icon :class=\"{ 'is-active': isActive.heading({ level: 1 }) }\" @click=\"commands.heading({level: 1})\">\r\n                              <b> H1 </b>\r\n                            </v-btn>\r\n                            <v-btn text icon :class=\"{ 'is-active': isActive.bold() }\" @click=\"commands.bold\">\r\n                                <v-icon>mdi-format-bold</v-icon>\r\n                            </v-btn>\r\n\r\n                            <v-btn text icon :class=\"{ 'is-active': isActive.underline() }\" @click=\"commands.underline\">\r\n                                <v-icon>mdi-format-underline</v-icon>\r\n                            </v-btn>\r\n                            <v-btn text icon @click=\"isTextfield=!isTextfield\">\r\n                                <v-icon>fas fa-edit</v-icon>\r\n                            </v-btn>\r\n                            <v-text-field v-if=\"isTextfield\" v-model=\"imageUrl\"></v-text-field>\r\n                            <v-btn v-if=\"isTextfield\" @click=\"loadImage(commands.image)\">\r\n                              add\r\n                            </v-btn>\r\n                          </v-container>\r\n                          <v-file-input hide-input accept=\"image/png, image/jpeg, image/bmp\" v-model=\"chosenfile\" @change=\"uploadImage(commands.image)\">                              \r\n                          </v-file-input>\r\n                        </div>   \r\n                    </editor-menu-bar>\r\n                  </v-row>\r\n                  <v-row>\r\n                      <v-col cols=12 >\r\n                          <editor-content class=\"editor-box\" :editor=\"editor\"/>\r\n                      </v-col>\r\n                  </v-row>\r\n                  <v-row>\r\n                    <v-container class=\"d-flex\">\r\n                      <v-combobox v-model=\"chips\" :items=\"items\" chips clearable\r\n                        label=\"Add hashtags\"\r\n                        multiple\r\n                        prepend-icon=\"mdi-filter-variant\"\r\n                        solo>\r\n                        <template v-slot:selection=\"{ attrs, item, select, selected }\">\r\n                          <v-chip v-bind=\"attrs\" :input-value=\"selected\" close @click=\"select\" @click:close=\"remove(item)\">\r\n                            <strong>{{ item }}</strong>&nbsp;                            \r\n                          </v-chip>\r\n                        </template>\r\n                      </v-combobox>\r\n                    </v-container>\r\n                  </v-row>\r\n                </v-container>\r\n              </v-container>\r\n            </v-col>\r\n            <v-col cols=\"12\" md=\"2\"></v-col>\r\n        </v-row>\r\n        <v-container >\r\n          <v-col align=\"center\">\r\n            <v-btn large rounded color=\"primary\" dark @click=\"updatePost\">\r\n              Publish\r\n            </v-btn>\r\n          </v-col>\r\n        </v-container>\r\n      </v-container>\r\n  </v-app>\r\n</template>\r\n\r\n<script>\r\nimport { Editor, EditorContent, EditorMenuBar  } from 'tiptap';\r\nimport { Heading, \r\n        Bold, \r\n        Underline,\r\n        Image } from 'tiptap-extensions';\r\nimport Header from '../../components/Header.vue'\r\nimport Bubble from '../../components/Bubble.vue'\r\nimport { mapGetters} from \"vuex\";\r\n// import router from '../router';\r\n// import { Heading, Underline} from 'tiptap-extensions';\r\nexport default {\r\n  components: {\r\n    EditorContent,\r\n    EditorMenuBar,\r\n    Header,\r\n    Bubble,\r\n  },\r\n  data() {\r\n    return {\r\n      editor: null,\r\n      isTextfield: false,\r\n      imageUrl: '',\r\n      chosenfile: null,\r\n      content2:'',\r\n      chips: [],\r\n      items: [],\r\n    }\r\n  },\r\n  async mounted() {\r\n    const queryString = window.location.search;\r\n    const urlParams = new URLSearchParams(queryString);\r\n    const postId = urlParams.get('postId')\r\n   \r\n    // let postId = this.$route.query.postId\r\n    const {data} = await this.$Axios.get('http://localhost:3000/api/post/'+postId);\r\n    this.content2 = data.content;\r\n    // console.log(\"data:\"+data);\r\n    this.editor = new Editor({\r\n      content: data.content,\r\n      extensions:[\r\n          new Heading({levels: [1,2,3]}),\r\n          new Bold(),\r\n          new Underline(),\r\n          new Image(),\r\n      ],\r\n      onUpdate:({getHTML}) => {\r\n        this.content2 = getHTML();\r\n        \r\n      }\r\n    });\r\n    this.getHashtags(postId);\r\n    \r\n  },\r\n  computed: {\r\n    ...mapGetters(['getUser', 'getStoryTitle','getStoryContent'])      \r\n  },\r\n  beforeUnmount() {\r\n    this.editor.destroy()\r\n  },\r\n  methods: {\r\n      loadImage:function(command){\r\n          command({src: this.imageUrl});\r\n          this.isTextfield = false;\r\n      },\r\n      async uploadImage(command) {\r\n        const formData = new FormData();\r\n        formData.append('image', this.chosenfile);\r\n\r\n        try {\r\n          const {data} = await this.$Axios.post('http://localhost:3000/api/users/upload/'.concat(this.getUser.id), formData, {\r\n            headers: {\r\n              'Content-Type': 'multipart/form-data'\r\n              }\r\n            });\r\n            // console.log('Write:' + data.photo);\r\n          command({src: data.photo});\r\n          // this.$store.dispatch('setAvatar', data.photo)\r\n        } catch(err) {\r\n          console.log(err);\r\n        }\r\n      },\r\n\r\n      async updatePost() {\r\n        const queryString = window.location.search;\r\n        const urlParams = new URLSearchParams(queryString);\r\n        const postId = urlParams.get('postId')\r\n        const body = {\r\n          title: this.getStoryTitle,\r\n          content: this.content2,\r\n          hashtags: this.chips\r\n        }\r\n        // console.log('http://localhost:3000/api/post/update/'+postId);\r\n        await this.$Axios.post('http://localhost:3000/api/post/update/'+postId, body)\r\n      },\r\n\r\n      remove (item) {\r\n        this.chips.splice(this.chips.indexOf(item), 1)\r\n        this.chips = [...this.chips]\r\n      },\r\n      // \"/api/hashtags/:postId\"\r\n      async getHashtags(postId) {\r\n        const {data} = await this.$Axios.get(\"http://localhost:3000/api/hashtags/\"+postId);\r\n        console.log(data);\r\n        this.chips = data;\r\n      }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n.editor-box> * {\r\n    border-color: grey;\r\n    border-style: solid;\r\n    border-width: 1px;\r\n}\r\n\r\n.is-active{\r\n    border-color: grey;\r\n    border-style: solid;\r\n    border-width: 1px;\r\n}\r\n/* Basic editor styles */\r\n.ProseMirror {\r\n  > * + * {\r\n    margin-top: 0.75em;\r\n  }\r\n}\r\n</style>"],"sourceRoot":"src/views/story"}]}
,CAAA,UAAA,IAAA,CAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,IAAA;AACA,OAHA;AAIA,KAzCA;AA0CA;AACA,IAAA,YA3CA,wBA2CA,UA3CA,EA2CA;AAAA;;AACA,UAAA,IAAA,GAAA;AACA,QAAA,WAAA,EAAA;AADA,OAAA;AAGA,WAAA,MAAA,CAAA,IAAA,CAAA,gBAAA,KAAA,OAAA,CAAA,EAAA,GAAA,SAAA,EAAA,IAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,GAAA,IAAA,IAAA,EAAA,MAAA,CAAA,QAAA,GAAA,IAAA;AACA,OAFA;AAGA,KAlDA;AAmDA;AACA,IAAA,cApDA,0BAoDA,UApDA,EAoDA;AAAA;;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,gBAAA,KAAA,OAAA,CAAA,EAAA,GAAA,UAAA,GAAA,UAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,GAAA,IAAA,QAAA,EAAA,MAAA,CAAA,QAAA,GAAA,KAAA;AACA,OAFA;AAGA,KAxDA;AA0DA;AACA,IAAA,cA3DA,0BA2DA,UA3DA,EA2DA;AAAA;;AACA,UAAA,KAAA,eAAA,IAAA,IAAA,EAAA;AACA,aAAA,MAAA,CAAA,GAAA,CAAA,gBAAA,KAAA,OAAA,CAAA,EAAA,GAAA,UAAA,GAAA,UAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,cAAA,GAAA,CAAA,IAAA,CAAA,GAAA,IAAA,OAAA,EAAA,MAAA,CAAA,QAAA,GAAA,IAAA;AACA,SAFA;AAGA;AACA,KAjEA;AAkEA;AACA,IAAA,aAnEA,yBAmEA,MAnEA,EAmEA;AAAA;;AACA,UAAA,KAAA,eAAA,IAAA,IAAA,EAAA;AACA;AACA,YAAA,KAAA,IAAA,CAAA,OAAA,IAAA,KAAA,OAAA,CAAA,EAAA,EAAA,KAAA,MAAA,GAAA,IAAA,CAAA,KACA;AACA,eAAA,MAAA,CAAA,GAAA,CAAA,gBAAA,KAAA,OAAA,CAAA,EAAA,GAAA,aAAA,GAAA,MAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,gBAAA,GAAA,CAAA,IAAA,CAAA,GAAA,IAAA,OAAA,EAAA,MAAA,CAAA,MAAA,GAAA,IAAA,CAAA,KACA,MAAA,CAAA,MAAA,GAAA,KAAA;AACA,WAHA;AAIA;AACA,OATA,MASA,KAAA,MAAA,GAAA,IAAA;AACA,KA9EA;AAgFA,IAAA,WAhFA,uBAgFA,MAhFA,EAgFA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,MAAA,CAAA,GAAA,CAAA,wBAAA,MAAA,CADA;;AAAA;AAAA;AACA,gBAAA,IADA,yBACA,IADA;AAEA;AACA,gBAAA,MAAA,CAAA,KAAA,GAAA,IAAA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA,KApFA;AAsFA,IAAA,iBAtFA,6BAsFA,EAtFA,EAsFA;AACA,UAAA,KAAA,YAAA,CAAA,EAAA,KAAA,WAAA,IAAA,KAAA,YAAA,CAAA,EAAA,KAAA,KAAA,EACA,KAAA,YAAA,CAAA,EAAA,IAAA,IAAA,CADA,KAGA,KAAA,YAAA,CAAA,EAAA,IAAA,KAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,EAAA,GAAA,IAAA,GAAA,KAAA,YAAA,CAAA,EAAA,CAAA;AACA,KA5FA;AA8FA,IAAA,aA9FA,2BA8FA;AACA,WAAA,SAAA,GAAA,EAAA;AACA,KAhGA;AAiGA;AACA,IAAA,aAlGA,yBAkGA,MAlGA,EAkGA,QAlGA,EAkGA,KAlGA,EAkGA,aAlGA,EAkGA;AAAA;;AACA,UAAA,IAAA,GAAA;AACA,QAAA,QAAA,EAAA,QADA;AAEA,QAAA,MAAA,EAAA,KAAA,OAAA,CAAA,EAFA;AAGA,QAAA,KAAA,EAAA,KAHA;AAIA,QAAA,OAAA,EAAA,KAAA,SAJA;AAKA,QAAA,cAAA,EAAA;AALA,OAAA;AAOA,WAAA,MAAA,CAAA,IAAA,CAAA,kBAAA,MAAA,EAAA,IAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,GAAA,IAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,WAAA,CAAA,MAAA;;AACA,UAAA,MAAA,CAAA,IAAA,CAAA,cAAA,IAAA,CAAA;;AACA,UAAA,MAAA,CAAA,YAAA;AACA;AACA,OANA;AAOA,KAjHA;AAmHA,IAAA,WAnHA,uBAmHA,MAnHA,EAmHA;AAAA;;AACA,WAAA,MAAA,CAAA,GAAA,CAAA,kBAAA,MAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,GAAA,IAAA,IAAA,EAAA;AACA,UAAA,OAAA,CAAA,QAAA,GAAA,EAAA;;AADA,qDAEA,GAAA,CAAA,IAAA,CAAA,IAFA;AAAA;;AAAA;AAEA,gEACA;AAAA,kBADA,OACA;;AACA,cAAA,OAAA,CAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AACA;AALA;AAAA;AAAA;AAAA;AAAA;AAMA,SAPA,CAQA;;AACA,OATA;AAUA;AA9HA;AArCA,CAAA","sourcesContent":["<template>\r\n    <v-app>\r\n        <Header/>\r\n        <!-- <SideMenu/> -->\r\n        <template v-if=\"loading\">\r\n            <v-progress-linear indeterminate color=\"yellow darken-2\"></v-progress-linear>\r\n        </template>\r\n        <v-container class=\"ma-6\">\r\n            <v-row class=\"ma-8\">  \r\n                <v-col align=\"center\" class=\"ma-6\">            \r\n                    <h1 style=\"border-margin: 10px\" v-html=\"post.title\"></h1>\r\n                </v-col>            \r\n            </v-row>\r\n            <v-row align=\"center\">\r\n                <v-col cols=\"12\" md=\"2\"></v-col>\r\n                <v-col cols=\"12\" md=\"6\" class=\"d-flex\">\r\n                    <v-avatar class=\"ma-2\">\r\n                        <v-img :src=\"post.photo\"></v-img>\r\n                    </v-avatar>\r\n                    <v-col cols=\"12\" md=\"6\">   \r\n                        <v-row class=\"ma-n1\" align=\"center\">                     \r\n                            {{post.name}}\r\n                            <!-- <v-spacer></v-spacer> -->\r\n                            <!-- <v-col v-if=\"showFollowBtn\">\r\n                                <v-btn v-if=\"!isFollow\" small class=\"ma-2\" outlined color=\"indigo\" @click=\"followAuthor(post.user_id)\">\r\n                                    follow\r\n                                </v-btn>\r\n                                <v-btn v-else small class=\"ma-2\" outlined color=\"indigo\" @click=\"unfollowAuthor(post.user_id)\">\r\n                                    unfollow\r\n                                </v-btn>\r\n                            </v-col> -->\r\n                            <!-- <v-spacer></v-spacer> -->\r\n                        </v-row>\r\n                        <v-row class=\"ma-n1\">\r\n                            <v-col cols=\"12\" md=\"10\">\r\n                                {{post.date}}\r\n                            </v-col>\r\n                        </v-row>\r\n                    </v-col>\r\n                </v-col>\r\n               \r\n            </v-row>\r\n     \r\n            <v-row>\r\n                <v-col cols=\"12\" md=\"2\"></v-col>\r\n                \r\n                <v-col cols=\"12\" md=\"7\" class=\"ml-8\">\r\n                    <v-row>\r\n                        <span class=\"ma-6\" v-html=\"post.content\"></span>\r\n                    </v-row>\r\n                    <v-row >\r\n                        <v-col>\r\n                            <v-container dense class=\"d-flex\">\r\n                                <v-col>\r\n                                    <v-container>\r\n                                        <h3>Hashtags: </h3>\r\n                                    </v-container>\r\n                                </v-col>\r\n                                <v-container>\r\n                                    <v-chip v-for=\"chip in chips\" :key=\"chip\" class=\"ma-2\">\r\n                                        {{chip}}\r\n                                    </v-chip>\r\n                                </v-container>\r\n                            </v-container>\r\n                        </v-col>\r\n                    </v-row>\r\n                    <v-row>\r\n                        <v-col cols=\"12\" md=\"6\" class=\"d-flex\">\r\n                            <v-container>\r\n                                <v-btn :disabled=\"isClap\" @click=\"createClap(post.id, 0)\">\r\n                                    <v-icon color=\"blue darken-2\">mdi-thumb-up</v-icon>                            \r\n                                </v-btn>\r\n                                <span class=\"ml-5\">{{post.clap_count}}</span>\r\n                            </v-container>\r\n                            <v-container>\r\n                                <v-btn @click=\"drawer = !drawer\">\r\n                                    <v-icon color=\"blue darken-2\">\r\n                                    mdi-message-text\r\n                                    </v-icon>\r\n                                </v-btn>\r\n                                <span class=\"ml-5\">{{post.response_count}}</span>\r\n                            </v-container>\r\n                        </v-col>\r\n                    </v-row>\r\n                    \r\n                </v-col>\r\n                <v-divider vertical></v-divider>\r\n                <v-col cols=\"12\" md=\"2\">\r\n                    <v-row align=\"center\">\r\n                        <v-col>\r\n                            <v-avatar size=\"80\" class=\"ma-2\">\r\n                                <v-img :src=\"post.photo\" height=\"80\"></v-img>                        \r\n                            </v-avatar>                            \r\n                        </v-col>                        \r\n                    </v-row>\r\n                    <v-col >\r\n                        <v-row class=\"ma-n1\"> <span class=\"black--text text-h5\">{{post.name}}</span></v-row>                       \r\n                        <v-row class=\"ma-n1\">\r\n                            <v-col cols=\"12\" md=\"10\">\r\n                                {{post.follower_count}} followers\r\n                            </v-col>\r\n                        </v-row>\r\n                        <v-row class=\"ma-n1\">\r\n                            <v-col cols=\"12\">\r\n                                {{post.bio}}\r\n                            </v-col>\r\n                        </v-row>\r\n                         <v-row class=\"ma-n1\"> \r\n                            \r\n                            <v-col v-if=\"showFollowBtn\">\r\n                                <v-btn v-if=\"!isFollow\" small class=\"ma-2\" outlined color=\"indigo\" @click=\"followAuthor(post.user_id)\">\r\n                                    follow\r\n                                </v-btn>\r\n                                <v-btn v-else small class=\"ma-2\" outlined color=\"indigo\" @click=\"unfollowAuthor(post.user_id)\">\r\n                                    unfollow\r\n                                </v-btn>\r\n                            </v-col>\r\n                            <v-spacer></v-spacer>\r\n                        </v-row>\r\n                    </v-col>\r\n                    <v-container>\r\n                        <v-col>\r\n                            <span>Related</span>\r\n                        </v-col>\r\n                        <v-divider></v-divider>\r\n                    </v-container>\r\n                </v-col>\r\n            </v-row>            \r\n        </v-container>\r\n        <v-navigation-drawer v-model=\"drawer\" temporary fixed right width=\"380\">\r\n            <v-list-item>\r\n                <v-list-item-avatar>\r\n                    <v-img :src=\"this.getUser.photo\"></v-img>\r\n                </v-list-item-avatar>\r\n\r\n                <v-list-item-content>\r\n                    <v-list-item-title class=\"text-h6\">{{this.getUser.username}}</v-list-item-title>\r\n                </v-list-item-content>\r\n            </v-list-item>\r\n            <v-container v-if=\"!isComment\">\r\n                <v-textarea auto-grow label=\"Comment\" solo outlined v-model=\"myComment\">                   \r\n                </v-textarea>\r\n                <v-row>\r\n                    <v-col align=\"center\">\r\n                        <v-btn small @click=\"removeComment()\">\r\n                            Cancel\r\n                        </v-btn>\r\n                        <v-btn small @click=\"createComment(post.id, 0, 0, post.response_count)\">\r\n                            Reply\r\n                        </v-btn>\r\n                    </v-col>\r\n                </v-row>\r\n            </v-container>\r\n            <v-divider></v-divider>\r\n\r\n            <v-list dense>\r\n                <v-list-item v-for=\"item in comments\" :key=\"item.comment.id\" link :style=\"'border-top: '+(item.comment.depth==1? 1 : 0)+'px solid grey'\">\r\n                    <Comment :commentData=\"item\" :post=\"post\" @msg=\"pushComment\"/>                    \r\n                    \r\n                </v-list-item>\r\n            </v-list>\r\n        </v-navigation-drawer>\r\n    </v-app>\r\n</template>\r\n\r\n<script>\r\nimport Header from '../../components/Header.vue'\r\nimport Comment from '../../components/Comment.vue'\r\n// import SideMenu from '../../components/SideMenu.vue'\r\nimport { mapGetters} from \"vuex\";\r\n\r\nexport default ({\r\n    \r\n    data() {\r\n        return {\r\n            post: {},\r\n            loading:true,\r\n            isFollow: false,\r\n            showFollowBtn: true,\r\n            isClap:false,\r\n            isComment: false,\r\n            chips:[],\r\n            drawer: false,\r\n            items: [\r\n                { title: 'Home', icon: 'mdi-view-dashboard' },\r\n                { title: 'About', icon: 'mdi-forum' },\r\n            ],\r\n            myComment:'',\r\n            comments: [],\r\n            isShowWindow: {}\r\n        }\r\n    },\r\n    async mounted() {        \r\n        if (this.getRefreshToken != null) {\r\n            const {data} = await this.$Axios.get(\"/api/users/me\")           \r\n            this.$store.dispatch('setUser', data);\r\n          \r\n        }\r\n        this.getPost();       \r\n    },\r\n    computed: {\r\n        ...mapGetters(['getUser', 'getRefreshToken'])\r\n    },\r\n    components: {\r\n        Header,\r\n        Comment,\r\n        // SideMenu,\r\n   },\r\n    methods: {\r\n        pushComment(data) {            \r\n            this.getComments(data.comment.post_id);\r\n            this.$forceUpdate();\r\n        },\r\n\r\n        getPost() {\r\n            const queryString = window.location.search;\r\n            const urlParams = new URLSearchParams(queryString);\r\n            const postId = urlParams.get('postId');\r\n            // const {data} = await this.$Axios.get('/api/post/'+postId);\r\n            this.$Axios.get('/api/post/'+postId).then((data)=> {\r\n                this.post = data.data;\r\n                this.loading = false;\r\n                if (this.getUser.id == 0) {\r\n                    this.showFollowBtn = false;\r\n                    this.isClap = true;\r\n                    this.isComment = true;\r\n                }\r\n                if (this.post.user_id == this.getUser.id) this.showFollowBtn = false;\r\n                else {\r\n                    this.checkFollowing(this.post.user_id);\r\n                }\r\n                this.getHashtags(postId);\r\n                this.checkPostClap(postId);\r\n                this.getComments(postId);\r\n            });\r\n            \r\n        },\r\n        //\"/api/users/:userId/favorites/create\"\r\n        // type==1 => comment\r\n        createClap(postId, type) {\r\n            const body = {\r\n                post_id: postId,\r\n                is_response: type\r\n            }\r\n            // console.log('/api/users/'+this.getUser.id+'/favorites/create');\r\n            this.$Axios.post('/api/users/'+this.getUser.id+'/favorites/create', body).then((res) => {                \r\n                if (res.data.msg == 'ok') this.post.clap_count += 1;\r\n                this.isClap = true;\r\n            });\r\n        },\r\n        //\"/api/users/:id/follow\"\r\n        followAuthor(followedId) {\r\n            const body = {\r\n                followed_id : followedId\r\n            }\r\n            this.$Axios.post('/api/users/'+this.getUser.id+'/follow', body).then((res) => {                \r\n                if (res.data.msg == 'ok') this.isFollow = true;\r\n            });\r\n        },\r\n        //\"/api/users/:id/follow/:target_id\"\r\n        unfollowAuthor(followedId) {\r\n            this.$Axios.delete('/api/users/'+this.getUser.id+'/follow/'+followedId).then((res) => {                \r\n                if (res.data.msg == 'delete') this.isFollow = false;\r\n            });\r\n        },\r\n\r\n        //\"/api/users/:id/follow/:target_id\"\r\n        checkFollowing(followedId) {\r\n            if (this.getRefreshToken != null) {\r\n                this.$Axios.get('/api/users/'+this.getUser.id+'/follow/'+followedId).then((res) => {                \r\n                    if (res.data.msg == 'exist') this.isFollow = true;\r\n                });\r\n            }\r\n        },\r\n        //\"/api/users/:userId/favorites/:postId\"\r\n        checkPostClap(postId) {\r\n            if (this.getRefreshToken != null) {\r\n                //if post author is me, hide clap button\r\n                if (this.post.user_id == this.getUser.id) this.isClap = true;\r\n                else {            \r\n                    this.$Axios.get(\"/api/users/\"+this.getUser.id+\"/favorites/\"+postId).then((res) => {\r\n                        if (res.data.msg == 'exist') this.isClap = true;\r\n                        else this.isClap = false;\r\n                    });\r\n                }\r\n            } else this.isClap = true;\r\n        },       \r\n\r\n        async getHashtags(postId) {\r\n            const {data} = await this.$Axios.get(\"/api/hashtags/post/\"+postId);\r\n            // console.log(data);\r\n            this.chips = data;\r\n        },\r\n\r\n        showCommentWindow(Id) {\r\n            if (this.isShowWindow[Id] == 'undefined' || this.isShowWindow[Id] == false) \r\n                this.isShowWindow[Id] = true; \r\n            else\r\n                this.isShowWindow[Id] = false\r\n            console.log(Id+': '+this.isShowWindow[Id]);\r\n        },\r\n\r\n        removeComment() {\r\n            this.myComment = '';\r\n        },\r\n        //\"/api/comment/:postId\"\r\n        createComment(postId, parentId, depth, responseCount) {\r\n            const body = {\r\n                parentId: parentId,\r\n                userId: this.getUser.id,\r\n                depth: depth,\r\n                content: this.myComment,\r\n                response_count: responseCount\r\n            };\r\n            this.$Axios.post(\"/api/comment/\"+postId, body).then((res) => {\r\n                    if (res.data.msg == 'ok') {\r\n                        this.getComments(postId);\r\n                        this.post.response_count += 1;                       \r\n                        this.$forceUpdate();\r\n                    }\r\n            })\r\n        },\r\n\r\n        getComments(postId) {\r\n            this.$Axios.get(\"/api/comment/\"+postId).then((res) => {\r\n                if (res.data.msg == \"ok\") {\r\n                    this.comments=[];\r\n                    for (let comment of res.data.data)\r\n                    {\r\n                        this.comments.push(comment);\r\n                    }\r\n                }\r\n                // console.log(this.comments);\r\n            });\r\n        }\r\n    }\r\n})\r\n</script>\r\n"],"sourceRoot":"src/views/story"}]}